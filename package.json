{
  "name": "kodepos-worker",
  "version": "1.0.0",
  "description": "Lightning-fast Indonesian postal code API with 83,761 complete records",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --env development",
    "dev:staging": "wrangler dev --env staging",
    "dev:production": "wrangler dev --env production",
    "deploy": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:development": "wrangler deploy --env development",
    "deploy:all": "npm run deploy:staging && npm run deploy",
    "build": "tsc --noEmit",
    "build:check": "tsc",
    "lint": "eslint src --ext .ts,.js --fix",
    "lint:check": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:create": "node scripts/setup-database.js production",
    "db:create:staging": "node scripts/setup-database.js staging",
    "db:create:dev": "node scripts/setup-database.js development",
    "db:migrate": "bash scripts/migrate.sh production",
    "db:migrate:staging": "bash scripts/migrate.sh staging",
    "db:migrate:dev": "bash scripts/migrate.sh development",
    "db:setup": "npm run db:create && npm run db:migrate",
    "db:setup:staging": "npm run db:create:staging && npm run db:migrate:staging",
    "db:setup:dev": "npm run db:create:dev && npm run db:migrate:dev",
    "db:info": "wrangler d1 info kodepos-db --json",
    "db:info:staging": "wrangler d1 info kodepos-db-staging --json",
    "import:data": "node scripts/import-postal-data.js",
    "health:prod": "KODEPOS_API_URL=https://kodepos-worker.tekipik.workers.dev curl -f $KODEPOS_API_URL/health || echo 'Production health check failed'",
    "health:staging": "KODEPOS_API_URL=https://kodepos-worker-staging.tekipik.workers.dev curl -f $KODEPOS_API_URL/health || echo 'Staging health check failed'",
    "health:dev": "KODEPOS_API_URL=http://localhost:8787 curl -f $KODEPOS_API_URL/health || echo 'Dev health check failed'",
    "health": "npm run health:prod",
    "config:validate": "node scripts/validate-config.js",
    "config:validate:all": "node scripts/validate-config.js --all-env",
    "config:setup:prod": "ENVIRONMENT=production node scripts/setup-environment.js",
    "config:setup:staging": "ENVIRONMENT=staging node scripts/setup-environment.js",
    "config:setup:dev": "ENVIRONMENT=development node scripts/setup-environment.js",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:check-consistency": "node scripts/validate-docs.js",
    "local": "npm run dev",
    "preview": "wrangler dev --env staging",
    "start": "npm run deploy",
    "prepare": "husky install",
    "precommit": "npm run type-check && npm run lint:check && npm run config:validate && lint-staged",
    "commit": "git-cz",
    "release": "semantic-release",
    "setup:local": "cp .env.example .env && npm install && npm run config:setup:dev && npm run db:setup:dev"
  },
  "keywords": [
    "indonesia",
    "postal-code",
    "api",
    "cloudflare-workers",
    "d1-database",
    "typescript",
    "rest-api",
    "geolocation",
    "indonesian-data",
    "kodepos"
  ],
  "author": {
    "name": "Maxwell Alpha",
    "email": "mxwllalpha@gmail.com",
    "url": "https://github.com/mxwllalpha"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mxwllalpha/kodepos-worker.git"
  },
  "bugs": {
    "url": "https://github.com/mxwllalpha/kodepos-worker/issues"
  },
  "homepage": "https://kodepos-worker.tekipik.workers.dev",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "hono": "^4.6.9",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231203.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.3.0",
    "vitest": "^4.0.14",
    "vitest-environment-miniflare": "^0.0.1",
    "wrangler": "^4.51.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "volta": {
    "node": "20.15.0",
    "npm": "10.7.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/mxwllalpha"
  }
}
